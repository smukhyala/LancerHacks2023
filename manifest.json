{
  "manifest_version": 2,
  "name": "GPT-3 for Google Docs",
  "version": "1.0",
  "description": "Use OpenAI's GPT-3 to generate text within Google Docs.",
  "permissions": ["https://docs.google.com/*"],
  "content_scripts": [
    {
      "matches": ["https://docs.google.com/*"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "GPT-3 for Google Docs"
  }
}
